{
  "name": "ghu",
  "displayName": "ghu",
  "version": "0.2.2",
  "description": "A promising build system.",
  "homepage": "https://larsjung.de/ghu/",
  "bugs": "https://github.com/lrsjng/ghu/issues",
  "author": "Lars Jung <lrsjng@gmail.com> (https://larsjung.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lrsjng/ghu.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node -r babel-core/register test/tests.js",
    "cover": "babel-node node_modules/.bin/isparta cover --dir coverage --report text --report html test/tests.js",
    "check": "npm run -s lint && npm run -s test",
    "babel": "rimraf es5 && babel -d es5 lib",
    "prepublish": "npm run -s check && npm run -s babel"
  },
  "main": "es5/ghu.js",
  "dependencies": {
    "autoprefixer": "6.1.2",
    "babel-core": "6.3.15",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "chalk": "1.1.1",
    "commander": "2.9.0",
    "cssmin": "0.4.3",
    "dateformat": "1.0.12",
    "escape-string-regexp": "1.0.3",
    "eslint": "1.10.3",
    "glob": "6.0.1",
    "html-minifier": "1.0.0",
    "includeit": "0.5.0",
    "jade": "1.11.0",
    "jszip": "2.5.0",
    "less": "2.5.3",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "rimraf": "2.4.4",
    "uglify-js": "2.6.1",
    "watch": "0.16.0",
    "webpack": "1.12.9"
  },
  "devDependencies": {
    "babel-cli": "6.3.15",
    "isparta": "4.0.0",
    "scar": "0.8.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
